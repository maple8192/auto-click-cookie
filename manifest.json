{
  "manifest_version": 2,
  "name": "Auto Clicker",
  "version": "1.0",
  "description": "A extension clicks cookies automatically",
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_icon": {
      "19": "icon.png"
    },
    "default_title": "",
    "default_popup": "popup/options.html"
  },
  "content_scripts": [
    {
        "matches": [ "https://orteil.dashnet.org/cookieclicker/", "https://natto0wtr.web.fc2.com/CookieClicker/" ],
        "js": [ "scripts/main.js" ]
    }
  ]
}